<h1>Create New Post</h1>

<%= form_with model: @post ,local: true , url: post_index_path do |f| %>
   <div>
     <%= f.label :title %><br/>
     <%= f.text_field :title%>
   </div>
    <div>
     <%= f.label :body %><br>
     <%= f.text_area :body%>
   </div>

   <div>
   <%= f.submit %>
   </div>
<% end %>
